<div class="undernav">

  <h1>
    This is the Login Page
  </h1>

  <%= form_with scope: :session, url: sessions_path, local: true do |form| %>
  <p>
    <%= form.label :Username %><br>
    <%= form.text_field :uname %>
  </p>

  <p>
    <%= form.label :Password %><br>
    <%= form.text_field :password %>
  </p>

  <p>
    <%= form.submit "Login" %>
  </p>
<% end %>

<%=link_to "Sign Up here if you are not an existing user", new_user_path %>

</div>
