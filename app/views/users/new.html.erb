<div class="undernav">

  <h1>
    This is the Sign Up Page
  </h1>

  <%= form_with scope: :user, url: users_path, local: true do |form| %>
  <p>
    <%= form.label :Username %><br>
    <%= form.text_field :uname %>
  </p>

  <p>
    <%= form.label :Password %><br>
    <%= form.text_field :password %>
  </p>

  <p>
    <%= form.label :Email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

</div>
